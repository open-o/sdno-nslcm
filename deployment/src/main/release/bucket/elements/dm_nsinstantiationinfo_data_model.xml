<?xml version="1.0" encoding="utf-8"?>
<inventory xmlns="http://www.w3school.com.cn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.w3school.com.cn inv-datamodel-1.0.xsd">
    <datamodel name="inv_nsinstantiationinfo">
        <index name="idx_nsinstantiationinfo_uniqueid" value="uuid" isunique="true" />
        <filter name="default"
            value="uuid in (:uuid) and instanceId in (:instanceId) and name in (:name) and value in (:value) and actionState in (:actionState)" />
    </datamodel>
</inventory>
